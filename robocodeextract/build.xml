<?xml version="1.0" encoding="UTF-8"?>

<!-- ====================================================================== -->
<!--  Apache Ant build script for building the Robocode extract             -->
<!-- ====================================================================== -->

<project name="robocodeextract" default="build" basedir=".">
  <description>Apache Ant build script for building the Robocode extract</description>

  <!-- Enable debug information when compiling -->
  <property name="debug" value="on"/>

  <!-- Import common build file -->
  <import file="../build/build.xml"/>

  <!-- ====================================== -->
  <!-- Target: build                          -->
  <!-- ====================================== -->
  <target
      name="build"
      description="Build the Robocode extract">

    <!-- Build extract.jar -->
    <antcall target="extract.jar"/>
  </target>

</project>